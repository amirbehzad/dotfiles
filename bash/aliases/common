# Common alias definitions

alias ll='ls -alh'
alias dir='ls -1Fa | grep "/$" | sort -f'
alias grep='grep -i --color'
alias hex='od -x'
alias df='df -kh'
alias s='ssh'
alias w='wget -c'
alias v='vim'
alias ps?='ps auxf | grep'
alias k='kill -9'
alias ..='cd ..'

# yeah, i miss dos :p
alias cls='clear'

# typo handling
alias got='git'

alias serveme='python -m SimpleHTTPServer'

